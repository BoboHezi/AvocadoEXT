{
	"name": "Avocado",
    "version": "1.0",
    "description": "A test Extension, created by avocado",
    "permissions":
    [
    	"activeTab",
    	"declarativeContent",
    	"webRequest",
		  "webRequestBlocking",
    	"storage",
    	"contextMenus",
    	"https://raw.githubusercontent.com/BoboHezi/"
    ],

    "content_scripts":
    [
    	{
    		"matches": ["http://www.baidu.com/", "https://www.baidu.com/"],
    		"js": ["content.js"],
    		"run_at": "document_start"
    	}
    ],

    "web_accessible_resources": ["avocado/extra.js", "avocado/index.js"],

    "optional_permissions":
    [
    	"tabs",
    	"http://www.google.com/"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "options_page": "options.html",
    "page_action": {
      "default_popup": "avocado/index.html",
      "default_title": "Avocado Eli",
      "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      }
    },
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "manifest_version": 2
}